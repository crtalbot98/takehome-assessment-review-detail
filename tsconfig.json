{
  "compilerOptions": {
    "target": "es6",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outDir": "build",
    "moduleResolution": "node",
    "baseUrl": ".",
    "skipLibCheck": true,
    "jsx": "react",
    "esModuleInterop": true,
    "module": "commonjs",
    "strict": true,
    "resolveJsonModule": true
  },
  "include": ["**/*",],
  "exclude": ["**/*.spec.ts"]
}